{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint --filter=alpinecss",
    "format": "prettier --write \"**/*.{ts,tsx,md,css}\"",
    "format:check": "prettier --list-different \"**/*.{ts,tsx,md,css}\"",
    "test:a11y": "turbo test:a11y",
    "test:a11y:ci": "turbo test:a11y:ci",
    "test:visual_regression": "turbo test:visual_regression",
    "test:visual_regression:ci": "turbo test:visual_regression",
    "test:visual_regression:update": "turbo test:visual_regression:update",
    "test:visual_regression:update:ci": "turbo test:visual_regression:update"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^2.2.3"
  },
  "name": "with-vite",
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/docs",
    "packages/alpinecss"
  ]
}
